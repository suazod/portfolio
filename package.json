{
  "homepage": "https://suazod.github.io/danny-suazo-portfolio",
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "bootstrap": "^4.6.0",
    "jquery": "^3.6.0",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.16.1",
    "react": "^18.0.0",
    "react-ga": "^2.7.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.0"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "watch-css": "sass --watch --update --style=expanded src/styles:src/styles",
    "build-css": "sass --no-source-map --style=compressed src/styles:src/styles",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all -p build-css && react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^2.2.0",
    "sass": "^1.49.10"
  }
}